{
  "name": "parsec-ts",
  "version": "0.0.0",
  "description": "[experimental] Parser combinator implementation for TypeScript",
  "main": "dist/index.js",
  "scripts": {
    "postinstall": "typings install",
    "build": "tsc",
    "watch": "tsc --watch",
    "lint": "tslint src/**/*.ts",
    "pretest": "npm run build",
    "coverage:pre": "npm run build",
    "coverage:body": "istanbul cover _mocha dist/**/test_*.js",
    "coverage": "npm-run-all coverage:pre coverage:body",
    "test": "mocha dist/**/test_*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Kuniwak/parsec-ts.git"
  },
  "keywords": [
    "parsec"
  ],
  "author": "Kuniwak",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Kuniwak/parsec-ts/issues"
  },
  "homepage": "https://github.com/Kuniwak/parsec-ts#readme",
  "devDependencies": {
    "istanbul": "^0.4.3",
    "mocha": "^2.4.5",
    "npm-run-all": "^1.8.0",
    "tslint": "^3.8.0",
    "typescript": "^1.8.10",
    "typings": "^0.8.1"
  }
}
